server {
    listen 80 default_server;
    listen [::]:80 default_server;
    server_name *.twilightparadox.com;
    charset utf-8;
    client_max_body_size 128M;
    
    location / {
        include proxy_params;
        proxy_pass http://unix:/home/ubuntu/Youflix/youflix/django-vue/djangoAPI/run/djangoAPI.sock:/uri/;
    }

    location /static/ {
        alias /home/ubuntu/Youflix/youflix/django-vue/.static_root/;
    }
}
